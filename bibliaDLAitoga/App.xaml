<Application x:Class="bibliaDLAitoga.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:bibliaDLAitoga"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/DayLib;component/Full.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/MoningLib;component/Full.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/NightLib;component/Full.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
